{
    "version": 2,
    "sql": [
      {
        "engine": "postgresql",
        "schema": "./schemas",
        "queries": "./queries",
        "gen": {
          "go": {
            "package": "models",
            "out": ".",
            "sql_package": "pgx/v4",
            "emit_json_tags": true,
            "json_tags_case_style": "camel",
            "emit_empty_slices": true
          }
        }
      }
    ]
  }
  